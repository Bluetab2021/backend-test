{
  "name": "backend-test",
  "version": "1.0.0",
  "description": "Setup for nodejs backend.",
  "author": "devtobias",
  "main": "index.js",
  "license": "MIT",
  "keywords": [
    "nodejs",
    "backend"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development&& npm run webpack:build && cd ./build && node index.js",
    "build": "cross-env NODE_ENV=production&& npm run webpack:build && npm run docker:build",
    "dev": "cd ./build&& cross-env NODE_ENV=development nodemon index.js",
    "webpack:dev": "cross-env NODE_ENV=development webpack --watch",
    "webpack:build": "webpack",
    "lint": "eslint --fix --ext .ts src/",
    "docker:build": "docker build . -t infotition/test",
    "docker:start": "docker run -p 3000:3000 infotition/test",
    "docker:start:node": "node index",
    "docker:compose": "npm run webpack:build && docker-compose up --build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "mongoose": "^5.12.14"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.2",
    "webpack": "^5.39.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
