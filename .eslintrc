{
    "env": { "node": true, "commonjs": true },
    "parser": "@typescript-eslint/parser",
    "parserOptions": { "ecmaVersion": 12 },
    "plugins": ["@typescript-eslint"],
    "extends": [
      "airbnb-base",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-console": 0,
      "import/no-unresolved": 0
    }
  }
